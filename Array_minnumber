
public class Array_minnumber
{
  public static void main(String[] args)
  {
    int a[] = {13,5,6,7,667,8,33,55,65};
    //int max=a[0];
    int min=a[0];
    for (int i=1; i<a.length;i++)
    {
        //if (a[i]>max)
        if (min>a[i])
        
        
        {
            //max = a[i];
            min=a[i];
           
        }
    }
    
   // System.out.println("max is " + max);
    System.out.println("Minimum is " + min);
  }
}
